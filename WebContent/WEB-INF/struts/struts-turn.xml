			<result name="listBusiness">/WEB-INF/jsp/business/listBusiness.jsp</result>
			<result name="listUser">/WEB-INF/jsp/business/listUser.jsp</result>
			<result name="listUs">/WEB-INF/jsp/business/listUser.jsp</result>
			
